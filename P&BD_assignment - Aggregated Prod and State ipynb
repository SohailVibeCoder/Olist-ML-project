        "# 2. Convert Date/Time Columns to the correct format (datetime)\n",
        "# The dates are currently stored as 'object' (text/string), which prevents time-based math.\n",
        "\n",
        "date_columns = [\n",
        "    'order_purchase_timestamp',\n",
        "    'order_approved_at',\n",
        "    'order_delivered_carrier_date',\n",
